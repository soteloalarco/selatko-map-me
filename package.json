{
  "name": "selatko-map-me",
  "version": "1.0.0",
  "description": "Web Application with social media traits for logging visits in a map and sharing them with friends.",
  "main": "index.js",
  "scripts": {
    "install": "cd server && npm install && npm run build",
    "client": "node scripts/start-client.js",
    "server": "cd server && npm run start",
    "start-dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "build": "node scripts/build.js",
    "start": "npm run server",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"

  },
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soteloalarco/selatko-map-me.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/soteloalarco/selatko-map-me/issues"
  },
  "homepage": "https://github.com/soteloalarco/selatko-map-me#readme",
  "dependencies": {
    "concurrently": "^5.3.0",
    "mocha": "^8.2.0",
    "should": "^13.2.3",
    "supertest": "^5.0.0"
  }
}
